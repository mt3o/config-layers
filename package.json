  },
  "homepage": "https://github.com/mt3o/config-layers#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.5.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-plugin-doctest": "^1.1.1",
    "vitest": "^3.2.4"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
