  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/config-layers.js",
      "require": "./dist/config-layers.cjs"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
